type User {
  id: ID! @unique
  name: String!
  email: String!
  hash:String
  sessions:[Session!]!
}

type Session {
  id: ID! @unique
  user: User!
  created: DateTime!
  validTo: DateTime!
  closed: DateTime
  token: String!
}

type Country {
  id: ID! @unique
  name: String!
}

type City {
  id: ID! @unique
  name: String!
  country: Country!
  votes: [Vote!]!
}

type Vote {
  id: ID! @unique
  user: User!
  city: City!
}
